            "$ref": "integer"
          },
          "type": "array"
        }
      }
    },
    "IncidentResponse": {
      "properties": {
        "incident": {
          "type": "IncidentResponseType"
        }
      }
    },
    "IncidentResponseType": {
      "properties": {
        "closed_at": {
          "desc": "Timestamp of incident close",
          "type": "integer"
        },
        "id": {
          "desc": "Incident unique identifier",
          "type": "integer"
        },
        "incident_preference": {
          "desc": "Incident preference belonging to incident",
          "type": "integer"
        },
        "links": {
          "type": "IncidentLinksResponse"
        },
        "opened_at": {
